{
  "schemaVersion": 1,
  "id": "bushcraft",
  "version": "${version}",
  "name": "Bushcraft",
  "description": "Bushcraft is a low-tech tech mod that can help you survive and gather resources, especially in the early game.",
  "authors": ["jship"],
  "contact": {
    "homepage": "https://amertune.neocities.org/",
    "sources": "https://github.com/jshipley/Bushcraft"
  },
  "license": "MIT",
  "icon": "assets/bushcraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["com.jship.bushcraft.fabric.BushcraftFabric"],
    "client": ["com.jship.bushcraft.fabric.client.BushcraftFabricClient"],
    "fabric-datagen": ["com.jship.bushcraft.fabric.datagen.BushcraftDatagen"],
    "jade": ["com.jship.bushcraft.compat.jade.BushcraftJadePlugin"],
    "emi": ["com.jship.bushcraft.compat.emi.BushcraftEmiPlugin"],
    "jei_mod_plugin": ["com.jship.bushcraft.compat.jei.BushcraftJeiPlugin"],
    "rei_client": ["com.jship.bushcraft.fabric.compat.rei.BushcraftReiPlugin"]
  },
  "mixins": ["bushcraft.mixins.json"],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "geckolib": ">=4.7.5",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
